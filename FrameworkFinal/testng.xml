<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests" thread-count="5">
<listeners>
<listener class-name="academy.listeners"></listener>
<listener class-name="resouces.ExtentReporterNG"></listener>
</listeners>
  <test thread-count="5" name="Validating user Login">
    <classes>
      <class name="academy.LoginTest"/>
       </classes>
  </test>
  <test thread-count="5" name="Validating title">
    <classes>
      <class name="academy.Validatetitle"/>
       </classes>
  </test>
  <test thread-count="5" name="Validating Navbar">
    <classes>
      <class name="academy.ValidateNavbar"/>
      
    </classes>
  </test> 
</suite> 
